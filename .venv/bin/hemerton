#!/home/dm0817/PycharmProjects/hemerton-cli/.venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'hemerton==0.1','console_scripts','hemerton'
__requires__ = 'hemerton==0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('hemerton==0.1', 'console_scripts', 'hemerton')()
    )
